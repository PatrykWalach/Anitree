{
    "name": "anitree",
    "version": "0.6.5",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "npm run apollo && vue-cli-service build",
        "lint": "vue-cli-service lint",
        "jestTest": "vue-cli-service test:unit --maxWorkers=2",
        "jestUpdate": "vue-cli-service test:unit -u",
        "jestWatch": "vue-cli-service test:unit --watch",
        "jestCoverage": "vue-cli-service test:unit --maxWorkers=2 --coverage",
        "codecov": "codecov",
        "apollo": "apollo client:codegen --tagName=gql --target=typescript --useReadOnlyTypes --queries=src/**/*.{ts,tsx,vue}",
        "apolloWatch": "apollo client:codegen --tagName=gql --target=typescript --useReadOnlyTypes --watch --queries=src/**/*.{ts,tsx,vue}"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.28",
