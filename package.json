        "@vue/eslint-config-typescript": "^5.0.2",
        "@vue/test-utils": "^1.0.0-beta.32",
        "apollo": "^2.25.0",
        "codecov": "^3.6.5",
        "eslint": "^6.8.0",
        "eslint-plugin-graphql": "^3.1.1",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-vue": "^6.2.2",
        "jest-transform-graphql": "^2.1.0",
        "prettier": "^2.0.2",
        "sass": "^1.26.3",
        "sass-loader": "^8.0.2",
