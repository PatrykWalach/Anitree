        "@types/jest": "^25.1.4",
        "@types/throttle-debounce": "^2.1.0",
        "@typescript-eslint/eslint-plugin": "^2.25.0",
        "@typescript-eslint/parser": "^2.25.0",
        "@vue/cli-plugin-babel": "^4.2.3",
        "@vue/cli-plugin-eslint": "^4.2.3",
        "@vue/cli-plugin-pwa": "^4.2.3",
